<!DOCTYPE html>
<!-- Created By CodingLab - www.codinglabweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8">
    <!--<title> Responsive Sidebar Menu  | CodingLab </title>-->

    <script src="./js/jquery-3.6.0.min.js"></script>
    <link rel="stylesheet" href="../Css/style.css">
    <script src="./js/borrar.js"></script>
   <script src="./js/actualizar.js"></script>
    <!-- Boxicons CDN Link -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
    <link rel="shortcut icon" type="image/ico" href="Img/Favi.ico"/>
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
   </head>
   <title>Lineas Del Cliente</title>
<body>
  <div class="sidebar">
    <div class="logo_content">
      <div class="logo">
        <i class=''> <img src="../Css/la-red.png" alt="" height="50px" width="50px"></i>
        <div class="logo_name"></div>
      </div>
      <div>
        
      </div>
      <i class='bx bx-menu' id="btn" ></i>
    </div>
    <ul class="nav_list">
      <li>
          <i class='bx bx-search' ></i>
          <input type="text" placeholder="Search...">
        <span class="tooltip">Search</span>
      </li>
      <li>
        <a href="/Empleado">
          <i class='bx bx-grid-alt' ></i>
          <span class="links_name">Inicio</span>
        </a>
        <span class="tooltip">Inicio</span>
      </li>
      <li>
        <a href="/TbCliente">
          <i class='bx bx-user' ></i>
          <span class="links_name">Clientes</span>
        </a>
        <span class="tooltip">Clientes</span>
      </li>
      <li>
        <a href="/GenerarLi">
          <i class='bx bx-chat' ></i>
          <span class="links_name">Generar Lineas
             </span>
        </a>
        <span class="tooltip">Gnerar Lineas/span>
        <li>
          <a href="">
            <i class='bx bx-chat' ></i>
            <span class="links_name">Generar Cuentas</span>
          </a>
          <span class="tooltip">Generar Cuentas</span>
        </li>
     
    <div class="profile_content">
      <div class="profile">
        <div class="profile_details">
          <img src="profile.jpg" alt="">
          <div class="name_job">
            <div class="name">Prem Shahi</div>
            <div class="job">Web Designer</div>
          </div>
        </div>
        <i class='bx bx-log-out' id="log_out"> <a href="cerrar">Cerrar Sesion</a></i>
      </div>
    </div>
  </div>
  <div class="home_content">
    <div class="text"> 
     
    </div>
    <div>
      <input type="text" class="">

    </div>
    <div class="table_responsive">
      <table>
        
        <thead>
          <tr>
      
            <th>Codigo De Linea</th>
            <th>Nombre De La Linea</th>
            <th>Monto maximo del credito</th>
            <th>Plazo maximo del credito</th>
            <th>Accion</th>
            <th>Accion</th>
       


          </tr>
        </thead>
    
        <tbody>
          <% Datos.forEach(function (Datos){ %> 
          <tr>
            <td>
              <input type="text" name="clave" class="ape" placeholder="Documento Cliente" value="<%=Datos.codlinea%>">
            </td>
            <td>
              <input type="text" name="rol" class="correo" placeholder="Documento Cliente" value="<%=Datos.nomlinea%>">
            </td>
            <td>
              <input type="text" name="estado" class="celular" placeholder="Documento Cliente" value="<%=Datos.montomaxicredito%>">
            </td>
         <td>
              <input type="text" name="imagen" class="sexo" placeholder="Documento Cliente" value="<%=Datos.plazomaxcred%>">
            </td>
  
              <td>
              <span class="action_btn"> <a href="#">ACTUALIZAR</a> </span> 
            </td>
            <td>
              <span class="action_btn2"> <a href="#">ELIMINAR</a></span>
            </td>
          </tr>
          <%})%>
         </tbody>

      </table>
    </div>

    </div>

    


  <script>
   let btn = document.querySelector("#btn");
   let sidebar = document.querySelector(".sidebar");
   let searchBtn = document.querySelector(".bx-search");

   btn.onclick = function() {
     sidebar.classList.toggle("active");
     if(btn.classList.contains("bx-menu")){
       btn.classList.replace("bx-menu" , "bx-menu-alt-right");
     }else{
       btn.classList.replace("bx-menu-alt-right", "bx-menu");
     }
   }
   searchBtn.onclick = function() {
     sidebar.classList.toggle("active");
   }
  </script>

</body>
</html>
