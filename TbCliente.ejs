      <h4>Formulario Registro Cliente</h4>
      <form action="/frminsertar2" method="POST" autocomplete="off">
      <input class="controls" type="text" name="doccli" id="Documento" placeholder="Ingrese su Documento">
      <input class="controls" type="text" name="nomcli" id="Usuario" placeholder="Ingrese su Nombre">
      <input class="controls" type="text" name="apecli" id="Apellido" placeholder="Ingrese su Apellido">
      <input class="controls" type="email" name="correocli" id="Correo" placeholder="Ingrese su Correo">
      <input class="controls" type="text" name="cel" id="Cel" placeholder="Ingrese su numero celular">
      <select class="controls"  name="sexo">

        <option>F</option>
        
        <option>M</option>
        
   
        
        </select>
      <input class="controls" type="date" name="fecha" id="fecha" >
      <input class="botons" type="submit" value="Registrar">
    </form>
