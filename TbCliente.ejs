              </td>
              <td>
                <input type="text" name="rol" class="correo" placeholder="Documento Cliente" value="<%=Datos.correocli%>">
              </td>
              <td>
                <input type="text" name="estado" class="celular" placeholder="Documento Cliente" value="<%=Datos.celular%>">
              </td>
           <td>
                <input type="text" name="imagen" class="sexo" placeholder="Documento Cliente" value="<%=Datos.sexo%>">
              </td>
