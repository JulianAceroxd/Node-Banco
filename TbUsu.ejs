<!DOCTYPE html>
<!-- Created By CodingLab - www.codinglabweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8">
    <!--<title> Responsive Sidebar Menu  | CodingLab </title>-->

    <script src="./js/jquery-3.6.0.min.js"></script>
    <script src="./js/actualizar.js"></script>
    <script src="./js/borrar.js"></script>

    <link rel="stylesheet" href="./Css/style.css">
    <!-- Boxicons CDN Link -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <link rel="shortcut icon" type="image/ico" href="Img/Favi.ico"/>
     
   </head>
   <title>USUARIOS BANCSTUD</title>
<body>
  <div class="sidebar">
    <div class="logo_content">
      <div class="logo">
        <i class=''> <img src="./Css/la-red.png" alt="" height="50px" width="50px"></i>
        <div class="logo_name">BANCSTUD</div>
      </div>
      <div>
        
      </div>
      <i class='bx bx-menu' id="btn" ></i>
    </div>
    <ul class="nav_list">
      <li>
          <i class='bx bx-search' ></i>
          <input type="text" placeholder="Search...">
        <span class="tooltip">Search</span>
      </li>
      <li>
        <a href="/Admi">
          <i class='bx bx-grid-alt' ></i>
          <span class="links_name">Inicio</span>
        </a>
        <span class="tooltip">Inicio</span>
      </li>
      <li>
        <a href="/TbUsu">
          <i class='bx bx-user' ></i>
          <span class="links_name">Usuarios </span>
        </a>
        <span class="tooltip">Usuarios </span>
      </li>
    </ul>
    <div class="profile_content">
      <div class="profile">
        <div class="profile_details">
          <img src="profile.jpg" alt="">
          <div class="name_job">
            <div class="name">Prem Shahi</div>
            <div class="job">Web Designer</div>
          </div>
        </div>
        <i class='bx bx-log-out' id="log_out"> <a href="cerrar">Cerrar Sesion</a></i>
      </div>
    </div>
  </div>
  <div class="home_content">
  
    <section class="form-register">
      <h4>Formulario Registro</h4>
      <form action="/frminsertar" method="POST">
      <input class="controls" type="text" name="doccli" id="Documento" placeholder="Ingrese su Documento" required>
      <input class="controls" type="text" name="nomusu" id="Usuario" placeholder="Ingrese su Usuario" required>
      <input class="controls" type="password" name="clave" id="password" placeholder="Ingrese su password"required>
      <input class="controls" type="text" name="rol" id="Rol" placeholder="Ingrese su rol"required>
      <input class="controls" type="text" name="estado" id="Estado" placeholder="Ingrese Estado"required>
      <input class="controls" type="text" name="imagen" id="imagen" placeholder="INGRESE IMG"required>
      <input class="botons" type="submit" value="Registrar">
    </form>
    </section>
<section class="form-register">
  <h4>USUARIOS A VISUALIZAR</h4>

  
  <select class="controls"  name="tipo">

      <option>Empleado</option>
      
      <option>Cliente</option>

      <option>Administradpr</option>
      
 
      
      </select>
 
  <input class="botons" type="submit" value="VISUALIZAR USUARIOS">

</section>
    



  
    <div class="table_responsive">
        <table>
          
          <thead>
            <tr>
              <th>Documento</th>
              <th>Usuario</th>
              <th>Clave</th>
              <th>Rol</th>
              <th>Estado</th>
              <th>imagen</th>
              <th>Accion</th>
              <th>Accion</th>
            </tr>
          </thead>
      
          <tbody>
            <% Datos.forEach(function (Datos){ %> 
             
              
            <tr>
              <td>
                <input type="text"  name="doccli" class="doc" placeholder="Documento Cliente" value="<%=Datos.doccli%>" required></td>
              <td>
                <input type="text" name="nomusu" class="usu" placeholder="Nombre Usuario" value="<%=Datos.nomusu%>"required></td>
              <td>
                <input type="text" name="clave" class="cla" placeholder="Documento Cliente" value="<%=Datos.clave%>"required></td>
              <td>
                <input type="text" name="rol" class="rol" placeholder="Documento Cliente" value="<%=Datos.rol%>"required></td>
              <td>
                <input type="text" name="estado" class="estado" placeholder="Documento Cliente" value="<%=Datos.estado%>"required></td>
           <td>
                <input type="text" name="imagen" class="img" placeholder="Documento Cliente" value="<%=Datos.imagen%>"required></td>
              <td>
                <span class="action_btn"> <a href="#">ACTUALIZAR</a> </span> 
              </td>
              <td>
                <span class="action_btn2"> <a href="#">ELIMINAR</a></span>
              </td>
            </tr>
            <%})%>
           </tbody>

        </table>
      </div>
    </div>

    


  <script>
   let btn = document.querySelector("#btn");
   let sidebar = document.querySelector(".sidebar");
   let searchBtn = document.querySelector(".bx-search");

   btn.onclick = function() {
     sidebar.classList.toggle("active");
     if(btn.classList.contains("bx-menu")){
       btn.classList.replace("bx-menu" , "bx-menu-alt-right");
     }else{
       btn.classList.replace("bx-menu-alt-right", "bx-menu");
     }
   }
   searchBtn.onclick = function() {
     sidebar.classList.toggle("active");
   }
  </script>

</body>
</html>
